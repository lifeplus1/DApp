{
  "tailwindCSS.includeLanguages": {
    "javascript": "javascript",
    "html": "html",
    "css": "css"
  },
  "tailwindCSS.experimental.classRegex": [
    "tw`([^`]*)",
    "tw=\"([^\"]*)",
    "tw={\"([^\"}]*)",
    "tw\\.\\w+`([^`]*)",
    "tw\\(.*?\\)`([^`]*)"
  ],
  "css.validate": false,
  "scss.validate": false,
  "less.validate": false,
  "tailwindCSS.validate": true,
  "editor.quickSuggestions": {
    "strings": true
  },
  "tailwindCSS.emmetCompletions": true,
  "files.associations": {
    "*.css": "tailwindcss"
  }
}
